<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Contact_Helper.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Contact_Helper"
    xmlns:cont="clr-namespace:Contact_Helper.Contacts"
    xmlns:vcf="clr-namespace:Contact_Helper.VCF"
    
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
     Title="Contact"
     ContentTemplate="{DataTemplate cont:ContactListPage}"
     Route="Contact Page" />
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
 Title="VCF"
 ContentTemplate="{DataTemplate vcf:ContactPage}"
 Route="VcfPage" />
    <ShellContent
    Title="True Caller Login"
    ContentTemplate="{DataTemplate local:LoginPage}"
    Route="LoginPage" />

</Shell>
